#include <iostream>

using namespace std;

int main() {
    // Tentukan nilai untuk variabel-variabel ini
    string nama = "Ananta Eka A";
    string npm = "13.2022.1.01099";
    string jurusan = "Sistem Informatika";
    string tempatlahir = "Sidoarjo";
    int tahunLahir = 2004;
    int bulanLahir = 4; 
    int tanggalLahir = 12;

    // Tampilkan output
    cout << "Nama: " << nama << endl;
    cout << "NPM: " << npm << endl;
    cout << "Jurusan: " << jurusan << endl;
    cout << "Tempat Lahir: " << tempatlahir << endl;
    cout << "Tanggal Lahir: " << tanggalLahir << "/" << bulanLahir << "/" << tahunLahir << endl;

    
}
